<?xml version="1.0" encoding="UTF-8" ?>
<database>
   <name>*dbname*</name>
   <create>true</create>
   <overwrite>false</overwrite>
   <charset>utf8</charset>
   <table>
      <name>*dbprefix*bplog_logs</name>
      <declaration>
         <field>
            <name>id</name>
            <type>integer</type>
            <autoincrement>1</autoincrement>
            <default>0</default>
            <notnull>true</notnull>
            <length>11</length>
         </field>

         <field>
            <name>systole</name>
            <type>integer</type>
            <length>4</length>
            <default>0</default>
         </field>

         <field>
            <name>diastole</name>
            <type>integer</type>
            <length>4</length>
            <default>0</default>
         </field>

         <field>
            <name>pulse</name>
            <type>integer</type>
            <length>4</length>
            <default>0</default>
         </field>

         <field>
            <name>created</name>
            <type>integer</type>
            <length>8</length>
            <notnull>false</notnull>
            <default></default>
         </field>

         <field>
            <name>user_id</name>
            <type>text</type>
            <length>64</length>
            <notnull>true</notnull>
            <default></default>
         </field>

      </declaration>
   </table>
</database>
